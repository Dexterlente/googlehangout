import React from 'react';
import useCallStateStore from './callStateStore.js';
import * as constants from "./constants.js";

const MyComponent = () => {
  const { state, dispatch } = useCallStateStore();

  // Setter functions
  const setSocketId = (socketId) => {
    dispatch({ type: 'SET_SOCKET_ID', payload: socketId });
  };

  const setLocalStream = (stream) => {
    dispatch({ type: 'SET_LOCAL_STREAM', payload: stream });
  };

  const setAllowConnectionsFromStrangers = (allowConnection) => {
    dispatch({ type: 'SET_ALLOW_CONNECTIONS', payload: allowConnection });
  };

  const setScreenSharingActive = (screenSharingActive) => {
    dispatch({ type: 'SET_SCREEN_SHARING_ACTIVE', payload: screenSharingActive });
  };

  const setScreenSharingStream = (stream) => {
    dispatch({ type: 'SET_SCREEN_SHARING_STREAM', payload: stream });
  };

  const setRemoteStream = (stream) => {
    dispatch({ type: 'SET_REMOTE_STREAM', payload: stream });
  };

  const setCallState = (callState) => {
    dispatch({ type: 'SET_CALL_STATE', payload: callState });
  };

  // Your component logic and JSX code here...

  return (
    <div>
      {/* Your JSX code using the state variables and their respective setter functions */}
    </div>
  );
};

export default MyComponent;
